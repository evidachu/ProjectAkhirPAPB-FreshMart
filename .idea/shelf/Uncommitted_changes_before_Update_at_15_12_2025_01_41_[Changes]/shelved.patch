+package com.papb.projectakhirandroid.navigation.screen
+
+// Mengubah Screen menjadi object karena sekarang semua anggota adalah data object
+sealed class Screen(val route: String) {
+    // --- Rute Root Navigation ---
+    data object Splash : Screen("splash_screen")
+    data object OnBoarding : Screen("onboarding_screen")
+    data object Main : Screen("main_root") // <-- Rute untuk MainNavGraph
+
+    // --- Rute Screens Utama ---
+    data object EditProfile : Screen("edit_profile_screen")
+    data object Checkout : Screen("checkout_screen")
+    data object Invoice : Screen("invoice_screen")
+
+    // Rute Detail Produk (menerima ID produk)
+    data object Details : Screen("details_screen/{productId}") {
+        // Fungsi helper untuk membangun rute dengan ID (Ditempatkan di dalam Details)
+        fun createRouteWithProductId(productId: Int): String {
+            return "details_screen/$productId"
+        }
+    }
+
+    // Rute Search (menerima query, opsional)
+    data object Search : Screen("search_screen?query={query}")
+
+    // Rute Daftar Produk (menerima judul kategori/list)
+    data object ProductList : Screen("product_list_screen/{title}")
+
+    // Rute Komunitas dengan Argument Tipe
+    data object AddPost : Screen("add_post_screen/{type}") {
+        fun createRoute(type: String) = "add_post_screen/$type"
+    }
+
+    // NOTE: Fungsi createRouteWithProductId dihapus dari luar,
+    // karena sudah ada di dalam data object Details.
+}
