+package com.papb.projectakhirandroid.presentation.screen.komunitas
+
+import androidx.lifecycle.ViewModel
+import androidx.lifecycle.viewModelScope
+import com.papb.projectakhirandroid.data.model.Post
+import kotlinx.coroutines.flow.MutableStateFlow
+import kotlinx.coroutines.flow.StateFlow
+import kotlinx.coroutines.flow.asStateFlow
+import kotlinx.coroutines.flow.update
+import kotlinx.coroutines.launch
+
+// Jika Anda menggunakan Hilt, tambahkan @HiltViewModel dan inject dependensi
+// import dagger.hilt.android.lifecycle.HiltViewModel
+// import javax.inject.Inject
+
+// @HiltViewModel
+class KomunitasViewModel : ViewModel() { // Jika menggunakan Hilt, hapus constructor jika tidak ada injection
+
+    // StateFlow untuk menyimpan daftar semua postingan
+    private val _posts = MutableStateFlow<List<Post>>(emptyList())
+    val posts: StateFlow<List<Post>> = _posts.asStateFlow()
+
+    /**
+     * Menambahkan postingan baru ke daftar.
+     */
+    fun addPost(newPost: Post) {
+        // Melakukan update pada StateFlow di coroutine scope
+        viewModelScope.launch {
+            _posts.update { currentPosts ->
+                // Menambahkan postingan baru di awal daftar
+                listOf(newPost) + currentPosts
+            }
+        }
+    }
+}
